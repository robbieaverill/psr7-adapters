{
    "name": "robbie/psr7-adapters",
    "description": "PSR-7 compliant adapters for SilverStripe HTTP classes",
    "type": "silverstripe-vendormodule",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/framework": "^4.0",
        "guzzlehttp/psr7": "^1.3 || ^2.0"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Robbie\\Psr7\\": "src/",
            "Robbie\\Psr7\\Tests\\": "tests/"
        }
    },
    "extra": [],
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
